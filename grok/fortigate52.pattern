FORTIDATE %{YEAR}\-%{MONTHNUM}\-%{MONTHDAY}
FORTISTRING (%{QS}|%{USER})
FORTIGATE_52BASE <%{NUMBER:syslogIndex}>date=%{FORTIDATE:date} time=%{TIME:time} devname=%{HOSTNAME:dev_name} devid=%{HOSTNAME:dev_id} logid=%{NUMBER:log_id} type=%{WORD:log_type} subtype=%{WORD:log_subtype} level=%{WORD:level} vd=%{FORTISTRING:virtual_domain}
FORTIGATE_52IPS srcip=%{IP:src_ip}( srcname=%{QS:src_name})?( srcport=%{INT:src_port})? srcintf=%{FORTISTRING:in_device} dstip=%{IP:dst_ip}( dstport=%{INT:dst_port})? dstintf=%{FORTISTRING:out_device}( poluuid=%{UUID:uuid})? sessionid=%{INT:session_id} proto=%{INT:proto} action=%{DATA:action} policyid=%{INT:policy_id}( dstcountry=%{QS:dst_country} srccountry=%{QS:src_country})?( trandisp=%{WORD:trans_display})?( transip=%{IP:trans_ip} transport=%{INT:trans_port})?( service=%{QS:service})?( app=%{QS:application})?( duration=%{INT:duration})?( sentbyte=%{INT:sent_byte})?( rcvdbyte=%{INT:received_byte})?( sentpkt=%{INT:sent_packet})?( rcvdpkt=%{INT:received_packet})?( vpn=%{QS:vpn_name} vpntype=%{DATA:vpn_type})? appcat=%{QS:application_category}( crscore=%{NUMBER:critical_score} craction=%{NUMBER:critical_action} crlevel=%{WORD:critical_level})? devtype=%{QS:dev_type}( osname=%{QS:os})?( osversion=%{QS:os_version})?( unauthuser=%{QS:auth_user} unauthusersource=%{QS:auth_source})? mastersrcmac=%{MAC} srcmac=%{MAC}
FORTIGATE_52AUTH logdesc=%{QS:log_description} srcip=%{IP:src_ip} dstip=%{IP:dst_ip} policyid=%{INT:policy_id} user=%{QS:user} group=%{QS:group} authproto=%{QS:proto} action=%{WORD:action} status=%{WORD:status} reason=%{QS:season} msg=%{QS:msg}
FORTIGATE_52DHCP logdesc=%{QS:log_description} interface=%{FORTISTRING:in_device} dhcp_msg=%{QS:dhcp_msg} mac=%{MAC:dhcp_mac} ip=%{IP:dhcp_ip} lease=%{INT:dhcp_lease} hostname=%{QS:dhcp_client} msg=%{QS:msg}
FORTIGATE %{FORTIGATE_52BASE} (%{FORTIGATE_52IPS}|%{FORTIGATE_52AUTH}|%{FORTIGATE_52DHCP})